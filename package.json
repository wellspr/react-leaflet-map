{
    "name": "@wellspr/react-leaflet-map",
    "private": false,
    "version": "1.0.2",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "sass": "sass ./sass/main.scss ./src/css/index.css --watch"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/react-leaflet-map.umd.cjs",
    "module": "./dist/react-leaflet-map.js",
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "require": "./dist/react-leaflet-map.umd.cjs",
            "import": "./dist/react-leaflet-map.js"
        },
        "./style.css": "./dist/style.css"
    },
    "types": "./dist/types/index.d.ts",
    "dependencies": {
        "leaflet": "^1.9.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sass": "^1.69.0"
    },
    "devDependencies": {
        "@types/leaflet": "^1.9.5",
        "@types/node": "^20.6.3",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react": "^4.0.3",
        "eslint": "^8.45.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "path": "^0.12.7",
        "typescript": "^5.0.2",
        "vite": "^4.4.5",
        "vite-plugin-dts": "^3.5.4",
        "vite-tsconfig-paths": "^4.2.1"
    },
    "peerDependencies": {
        "@types/leaflet": "^1.9.5",
        "leaflet": "^1.9.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wellspr/react-leaflet-map"
    },
    "keywords": [
        "react",
        "leaflet",
        "maps"
    ],
    "author": "Paulo Wells",
    "homepage": "https://github.com/wellspr/react-leaflet-map#react-leaflet-map"
}
